{
    "name": "saritasa/eloquent-custom",
    "type": "library",
    "description": "Saritasa customizations for Eloquent",
    "keywords": ["laravel", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^5.4 || ^6.0 || ^9.0",
        "illuminate/notifications": "^5.4 || ^6.0 || ^9.0",
        "illuminate/support": "5.* || ^6.0 || ^9.0",
        "saritasa/php-common": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\Database\\Eloquent\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\Database\\Eloquent\\PredefinedMigrationsServiceProvider"
            ]
        }
    }
}
